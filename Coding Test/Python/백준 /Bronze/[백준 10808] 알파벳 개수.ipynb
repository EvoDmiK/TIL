   "source": [
    "string   = input()\n",
    "alphacnt = {alpha : 0 for alpha in ascii_lowercase}\n",
    "\n",
    "for k, v in Counter(string).items():\n",
    "    alphacnt[k] += v\n",
    "\n",
    "print(' '.join([str(v) for v in alphacnt.values()]))"
   ]
