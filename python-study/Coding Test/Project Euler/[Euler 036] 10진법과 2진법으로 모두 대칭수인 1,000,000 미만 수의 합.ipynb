    "sum = 0\n",
    "for num in range(1000000):\n",
    "\n",
    "    binary = bin(num)[2:]\n",
    "    num    = str(num)\n",
    "\n",
    "    condition = [binary == binary[::-1], num == num[::-1]]\n",
    "\n",
    "    if all(condition): sum += int(num)\n",
    "\n",
    "print(sum)"
   ]
